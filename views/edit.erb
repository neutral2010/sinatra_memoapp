<h1>Edit</h1>
<form action="/memos/<%= @memo[:id] %>" method="post">
  <input type="hidden" name="_method" value="PATCH" />
  <h2>Favorite</h2>
  <input type="text" value= "<%= @memo[:title] %>" name="title" id="title">
  <h2></h2>
  <h2>Description</h2>
  <textarea name="content" id="content"><%= @memo[:content] %></textarea>
  <div><input type="submit" value="変更"></div>
  <a href="/memos/<%= @memo[:id] %>">戻る</a>
</form>
